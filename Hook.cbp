<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Hook" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release_Xav">
				<Option output="bin\winsplithook.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="hook_src\obj\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="bin\WinSplit.exe" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Target title="Release_Raph">
				<Option output="bin\winsplithook.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="hook_src\obj\" />
				<Option type="3" />
				<Option compiler="mingw_compiler_43" />
				<Option host_application="bin\WinSplit.exe" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-DBUILD_DLL" />
			<Add option="-DUNICODE" />
		</Compiler>
		<Linker>
			<Add library="user32" />
		</Linker>
		<Unit filename="hook_src\main.cpp" />
		<Unit filename="hook_src\ressource.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
